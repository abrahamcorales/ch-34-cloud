{{> head }}
{{> nav }}
<div> 
<form class="ui form" action="/logout" method="post"  >
  <div class="field">
    <h1>bienvenido {{ name }} <button style="margin-left: 5%;" class="ui button" type="submit">Desloguearse</button></h1> 
  </div>
  
</form>
</div>
<div>
    <h1> PRODUCTOS</h1>
    <table class="ui celled table" style="text-align:center;" >
        <thead>
          
          <tr>
              {{#each keys }}  
             <th>{{this}}</th>
             {{/each }}
        </tr>
        
        </thead>
        <tbody>
            {{#each data }}
          <tr>
            <td data-label="{{this.nombre }}">{{this.nombre }}</td>
            <td data-label="{{this.precio }}">{{this.precio }}</td>
            <td data-label="{{this.imagen }}"><img src="{{ this.imagen }}" alt="" border=3 height=50 width=50></img></td>
          </tr>
            {{/each }}
        </tbody>
      </table>
</div>